{
  "name": "httpstatus",
  "version": "1.0.0",
  "description": "Utility to help test application's handling of HTTP status codes",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "run:dev": "nodemon dist/index.js",
    "run:prod": "node dist/index.js",
    "dev": "npm run build && concurrently \"npm run watch\" \"npm run run:dev\"",
    "start": "npm run run:prod",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MasterOdin/httpstatus.git"
  },
  "keywords": [
    "http",
    "http-status-code"
  ],
  "author": "Matthew Peveler <matt.peveler@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MasterOdin/httpstatus/issues"
  },
  "homepage": "https://github.com/MasterOdin/httpstatus#readme",
  "devDependencies": {
    "@types/express": "^4.17.12",
    "@types/js-yaml": "^4.0.1",
    "@types/node": "^10.17.60",
    "concurrently": "^6.2.0",
    "nodemon": "^2.0.7",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "express": "^4.17.1",
    "http-status-codes": "^2.1.4",
    "js-yaml": "^4.1.0"
  }
}
